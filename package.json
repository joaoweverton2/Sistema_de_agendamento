{
  "name": "agendamento-mercadorias",
  "version": "1.0.0",
  "description": "Sistema de agendamento de recebimento de mercadorias",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run copy-public",
    "start": "node dist/server.js",
    "copy-public": "mkdir -p dist/public && (cp -r public/ dist/ 2>/dev/null || echo 'No public directory found')"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.30",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}